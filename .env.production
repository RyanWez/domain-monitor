# Production Environment Variables for Fly.io
NODE_ENV=production
PORT=8080

# Database Configuration (will be set by Fly.io secrets)
# DB_HOST=
# DB_PORT=5432
# DB_NAME=
# DB_USER=
# DB_PASSWORD=

# JWT Secret (will be set by Fly.io secrets)
# JWT_SECRET=

# Monitoring Configuration
CHECK_INTERVAL_MINUTES=5

# Telegram Configuration (optional - can be set via app settings)
# TELEGRAM_BOT_TOKEN=
# TELEGRAM_CHAT_ID=

# CORS Origins
CORS_ORIGINS=https://domain-monitor-vercel.vercel.app,http://localhost:3000